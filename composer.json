    "nwidart/laravel-modules": "^3.3",
    "spring369/laravel-upload": "^1.0@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laravel": {
